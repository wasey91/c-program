#include<stdio.h>

int factorial(int number)
{
  if (number == 0)
    return 1;
  else
    return(number * factorial(number-1));
}

int main()
{
  int number;
  int fact;
  printf("Enter a number: ");
  scanf("%d", &number);

  fact = factorial(number);
  printf("Factorial %d" ,fact);
  return 0;
}
